<p id="notice"><%= notice %></p>

<h1>Character Locations</h1>

<table>
  <thead>
    <tr>
      <th>Coordinates</th>
      <th>Character img file</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @character_locations.each do |character_location| %>
      <tr>
        <td><%= character_location.coordinates %></td>
        <td><%= character_location.character_img_file %></td>
        <td><%= character_location.image_id %></td>
        <td><%= link_to 'Show', character_location %></td>
        <td><%= link_to 'Edit', edit_character_location_path(character_location) %></td>
        <td><%= link_to 'Destroy', character_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Character Location', new_character_location_path %>
